package arrytraaversl12;

import java.util.Scanner;

public class Occurance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
int size=sc.nextInt();
int a[]=new int[size];
for(int i=0;i<a.length;i++) {
	a[i]=sc.nextInt();
}int v=sc.nextInt();
System.out.println(method(a,v));
	}
	public static int method(int a[],int v) {
	int count=0;
	for(int i=0;i<a.length;i++) {
		if(a[i]==v) {
			count++;
		}
	}
	return count;
		}
}

