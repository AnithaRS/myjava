package IfElse2;
import java.util.Scanner;
public class DivBy3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
	    method(n);
	}
	public static void method(int n) {
	if(n%3!=0) {	
		System.out.println("no");
	}else {
		System.out.println("Yes");
	}
	}
	
	

}
