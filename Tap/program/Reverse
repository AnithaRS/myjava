package whileLoop10;

import java.util.Scanner;

public class Reverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
System.out.println(method(n));
	}
	public static int method(int n) {
		int rev=0;
		while(n>0) {
			int rem=n%10;
			rev=rem+(rev*10);
			n=n/10;
		}return rev;
	}

}
