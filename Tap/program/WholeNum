package ifelse4;

import java.util.Scanner;

public class WholeNum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		method(n);
	}
	public static void method(int n) {
		if(n<0) {
			System.out.println(-n);
		}else {
			System.out.println(n);
		}
	}

}
