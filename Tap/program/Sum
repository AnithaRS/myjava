package whileLoop10;

import java.util.Scanner;

public class Sum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
System.out.println(method(n));
	}
	public static int method(int a) {
	  int sum=0;

		while(a>0) {
			 int mod=a%10;
			sum=sum+mod;
			a=a/10;
		}return sum;
	}

}
