package Partten;

public class Partten3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=4;
		method(n);
	}
	public static void method(int n) {
		for(int i=0;i<n;i++) {
			for(int j=0;j<n-i;j++) {
				System.out.print("* ");
			}System.out.println();
		}
	}

}

