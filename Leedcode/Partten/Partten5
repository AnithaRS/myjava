package Partten;

public class Partten5 {

	public static void main(String[] args) {

		partten5(4);
	}
	public static void partten5(int n) {
		for(int i=1;i<=2*n;i++) {
			int t=i<=n?i:2*n-i;
			for(int j=1;j<=t;j++) {
				System.out.print("*");
			}System.out.println();

		}
	}

}
