package Partten;

public class Partten6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
partten6(4);
	}

	public static void partten6(int n) {
		for(int r=0;r<=n;r++) {
			int sp=n-r;
			for(int s=1;s<=sp;s++) {
				System.out.print(" ");
			}
			for(int c=1;c<=r;c++) {
				System.out.print("*");
			}System.out.println();
		}
	}
}





