package Partten;

public class partten11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		partten(4);
	}
	public static void partten(int n) {
		for(int i=0;i<n;i++) {
			
			int sp=i;
			for(int s=1;s<=sp;s++) {
				System.out.print(" ");
			}
			for(int j=1;j<=n-i;j++) {
				System.out.print("* ");
			}System.out.println();
		}
	}

}
