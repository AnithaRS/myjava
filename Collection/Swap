import java.util.*;
class Main {
    public static void main(String[] args) {
           List<String> l=new ArrayList<>();
           l.add("aaa");
           l.add("bbb");
           l.add("ccc");
        Collections.swap(l,0,2);
        System.out.println(l);
    }
}
  swap collection     [ccc, bbb, aaa]
